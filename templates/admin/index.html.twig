{# templates/admin/my_dashboard.html.twig #}
{% extends '@EasyAdmin/layout.html.twig' %}

{% block main %}

    <div class="container">
		<div class="col-md-4 card card-body">
            <h4>En attente d'access</h4>
            {% for user in new_users %}
            <a href="{{path('admin_user_edit',{'entityId':user.id})}}">{{user.email }}</a>
            {% else %}
                <p>aucun</p>
            {% endfor %}
        </div>
        <div class="col-md-8">
            {{ render_chart(chart) }}
        </div>
        
    </div>

{% endblock main %}